{
    "asi": true,
    "maxerr": 50,
    "bitwise": false,
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "esversion": 9,
    "forin": false,
    "freeze": true,
    "futurehostile": true,
    "latedef": true,
    "maxcomplexity": 10,
    "maxdepth": 3,
    "maxparams": 3,
    "maxstatements": 100,
    "nocomma": true,
    "nonbsp": true,
    "predef": ["module", "require", "console", "document", "window"],
    "shadow": "inner",
    "singleGroups": true,
    "strict": "implied",
    "undef": true,
    "unused": "vars"
}